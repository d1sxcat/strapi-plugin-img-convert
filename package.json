{
  "version": "2.0.0",
  "keywords": [
    "strapi",
    "strapi-plugin",
    "webp",
    "avif",
    "converter",
    "image"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "lint": "eslint --max-warnings=0"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.30",
    "@strapi/icons": "^2.0.0-rc.30",
    "react-intl": "^7.1.11"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.29.0",
    "@strapi/typescript-utils": "^5.29.0",
    "@types/react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "@types/react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "eslint": "^9.36.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.2",
    "yalc": "^1.0.0-pre.53"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "sharp": "^0.33.0",
    "styled-components": "^6.1.19"
  },
  "strapi": {
    "kind": "plugin",
    "name": "img-convert",
    "displayName": "Image Converter",
    "description": "Converts media uploads to any sharp supported format such as webp or avif"
  },
  "name": "strapi-plugin-img-convert",
  "description": "Converts media uploads to any sharp supported format such as webp or avif",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/d1sxcat/strapi-plugin-img-convert.git"
  },
  "bugs": {
    "url": "https://github.com/d1sxcat/strapi-plugin-img-convert/issues"
  },
  "homepage": "https://github.com/d1sxcat/strapi-plugin-img-convert#readme",
  "author": "d1sxcat <shaun.mills@live.com>"
}
